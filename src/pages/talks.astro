---
// talks.astro
import { getCollection } from "astro:content";
import Layout from "../layouts/Layout.astro";
import List from "../components/List.astro";
const items = (await getCollection("talks")).filter((x) => !x.data.hidden);
const contactText = "... your works intro ...";
---

<Layout pageType="talks">
    <List items={items} type="talks" ContactText={contactText} />
</Layout>
